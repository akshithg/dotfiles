Host *
	IdentityFile ~/.ssh/id_default

# Include ~/.ssh/config.d/* # doesn't work with VSCode Remote SSH :(


# git
Host github.com
	HostName github.com
	User akshithg

Host gitlab.com
	HostName gitlab.com
	User akshithg


# osu
Host osu-jump
	HostName access.engr.oregonstate.edu
	User gunaseka

Host osu.*
	User gunaseka
	ForwardAgent yes
	ProxyJump osu-jump
	RemoteCommand tmux -CC new -A -s 0 || exec $SHELL
	RequestTTY yes
	SendEnv MY_*

Host osu.cpu
	HostName babylon.eecs.oregonstate.edu

Host osu.gpu
	HostName pelican.eecs.oregonstate.edu

Host osu.hpc
	HostName submit.hpc.engr.oregonstate.edu

Host osu.answer
	HostName eng-answerlab.eecs.oregonstate.edu

Host osu.kernel
	Hostname fw-owyhee.eecs.oregonstate.edu
	LocalForward 7474 localhost:7474
	LocalForward 7687 localhost:7687



# cloudflare tunnels
Host zero.*
	ProxyCommand cloudflared access ssh --hostname %h
	ForwardAgent yes
	SendEnv MY_*

Host zero.desk
	HostName desk.akshithg.xyz
	User g


# ctf servers
Host pwncollege
	Hostname pwn.college
	User hacker
	IdentityFile ~/.ssh/id_pwncollege
